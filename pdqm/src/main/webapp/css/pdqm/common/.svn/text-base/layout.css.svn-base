@charset "utf-8";

/* default setting */
html{overflow-y:scroll;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,fieldset,p,span,address,em,blockquote{margin:0;padding:0;font-family:'돋움', Dotum !important;}

img, fieldset,legend{border:0 none;}
body, h1, h2, h3, h4, h5, dl, dt, dd, ul, ol, li, th, td, table, div, p, span, form, fieldset, legend, select, input, label, textarea{font-size:12px;font-family:'돋움', Dotum;color:#666;}
ul, ol, li {list-style:none outside;}
input, select, textarea {vertical-align:middle;font-size:12px;font-family:'돋움', Dotum;color:#666;}
address, em{font-style:normal;}
.png24{_tmp:expression(setPng24(this))}

/* 재설정 */
legend, caption{text-indent:-10000px;font-size:0;line-height:0;}
table{width:100%;border-collapse:collapse;table-layout:fixed;}

a{font-size:12px;font-family:'돋움', Dotum;color:#666;text-decoration:none;}
a:hover, a:active{color:#415aad;}

input.text{height:16px;line-height:16px;border:1px solid #cfcfcf;}
input.readonly{border:1px solid #d8d8d8;background-color:#f2f2f2;}
select, select.text{height:20px;line-height:20px;border:1px solid #cfcfcf;}
textarea.text{line-height:18px;margin:5px 5px 5px 0;padding:10px;border:1px solid #cfcfcf;}
.on{display:block !important;}

/* ============= layout setting ======================================= */
body{width:100%;background:url('/pdqm/images/pdqm/common/bg_body.gif') repeat-x 0 0;}

#skipNav {position:relative;width:986px; z-index:100;}
#skipNav a {display:block; overflow:hidden; position:absolute; left:0; top:0; width:100%; height:0; color:#fff; text-align:center;}
#skipNav a:hover, #skipNav a:focus, #skipNav a:active {background:#5c73c3; padding:8px 0 7px 0; height:11px; font-size:0.95em; line-height:11px; vertical-align:top;}

#wrapper{padding:0 14px;background:url('/pdqm/images/pdqm/common/bg_background.jpg') no-repeat 0 0;}
#header{width:986px;height:131px;}
#footWrap{position:relative;margin-top:15px;border-top:1px solid #e0e0e0;background-color:#f8f8f6;}
#conWrap{overflow:hidden;/*zoom:1;*/width:942px;padding:10px 22px;background:url('/pdqm/images/pdqm/common/bg_conWrap2.gif') repeat-y 0 0;}
#lnbArea{display:inline-block;_display:inline;float:left;width:183px;min-height:100%;height:100%;margin-right:12px;vertical-align:top;}
#conArea{position:relative;display:inline-block;_display:inline;float:left;width:727px;min-height:377px;padding-left:19px;padding-bottom:50px;border-left:1px solid #e9e9e9;}
#conBox{width:717px;margin:0 auto;line-height:18px;}

/* logo_toplink */
.topArea{overflow:hidden;height:53px;vertical-align:top;}
h1{float:left;margin:13px 0 0 10px;}
.toplink{float:right;margin:9px 7px 0 0;}
.toplink li{display:inline;}

/* GNB */
.gnbArea{position:relative;height:64px;background:url('/pdqm/images/pdqm/common/menu/bg_gnb.png') no-repeat 0 0;}
.gnbArea li{float:left;padding:0 35px;background:url('/pdqm/images/pdqm/common/menu/line_gnb.gif') no-repeat right 0;}
.gnbArea li.last{background:none;}
.gnbArea .submenu{display:none;position:absolute;top:38px;height:26px;padding-right:10px;background:url('/pdqm/images/pdqm/common/menu/bg_submenu.png') no-repeat 100% 0;}
.gnbArea .submenu ul{display:inline-block;_overflow:hidden;width:100%;height:100%;margin:0 10px 0 -10px;padding-left:10px;background:url('/pdqm/images/pdqm/common/menu/bg_submenu.png') no-repeat 0 0;}
.gnbArea .submenu li{_float:left;padding:5px 10px 0;_padding-top:5px;background:none;}
.gnbArea #top-sub-menu1{left:20px;_width:290px;}/* 공공정보품질관리 */
.gnbArea #top-sub-menu2{left:220px;_width:220px}/* 지식마당 */
.gnbArea #top-sub-menu3{left:370px;_width:520px}/* 지원창구 */
.gnbArea #top-sub-menu4{left:510px;_width:140px}/* 품질관리교육 */
.gnbArea #top-sub-menu5{left:680px;_width:230px}/* 소통참여 */
.gnbArea #top-sub-menu6{left:700px;_width:230px}/* 센터소개 */

/* secNavi */
#lnbArea h2{margin-left:7px;}
#lnbArea ul{width:100%;margin-top:21px;border-top:1px solid #e4e4e4;}
#lnbArea li{margin:0;padding:0;line-height:0;font-size:0;}
#lnbArea ul.depth3{width:154px;margin:0;padding:10px 10px 10px 15px;border:1px solid #ebebeb;background:#f8f8f8}
#lnbArea ul.depth3 li{padding:2px 0 2px 10px;background:url('/pdqm/images/pdqm/common/icon/bult_1.gif') no-repeat 0 center;}
#lnbArea li img, #lnbArea li a{margin:0;padding:0;line-height:0;font-size:0;}

/* footer */
.footer{width:746px;height:70px;padding:16px 0 0 240px;border-top:1px solid #fff;}
.footer ul li{display:inline;}
.footer address{margin:10px 0 5px;}
.footer .logo{position:absolute;top:21px;left:30px;}

/* ============== index ================================================= */
#indexArea{overflow:hidden;width:986px;}
.conBox{float:left;width:744px;height:431px;padding:18px 14px;background:url('/pdqm/images/pdqm/common/main/bg_conBox.gif') no-repeat 0 0;}
.conBox div{display:inline-block;_display:inline;float:left;vertical-align:top;}
.conBox .visual{width:265px;height:280px;}
.conBox .visual img{position:absolute;left:16px;}
.rBox{float:left;width:200px;margin-left:14px;}
/* 지식서비스바로가기 */
.service{height:285px;padding-left:14px;}
.service h2{margin:0 0 8px 10px;}
.service ul{width:449px;height:238px;padding-left:15px;background:url('/pdqm/images/pdqm/common/main/bg_serviceBox.gif') no-repeat 0 0;}
.service li{display:inline-block;_display:inline;}
*+html .service li{display:inline;}
.service li img{padding:16px 10px 0 0;filter:alpha(opacity:100,style:0);opacity:1;}
.service li a:hover img{filter:alpha(opacity:60,style:0);opacity:.60;}

/* popup zone */
.popzone{position:relative;width:262px;height:136px;}
.popzone .rolling{width:244px;margin:5px auto;padding-left:18px;}
.popzone ul.uxRoll{overflow:hidden;width:225px;margin:0 auto;}
.popzone ul.uxRoll li{float:left;}
.popzone ul.uxRoll li img{width:225px;height:110px;}
.popzone span.Lbtn1{position:absolute;left:0;top:60px;}
.popzone span.Rbtn1{position:absolute;right:0;top:60px;}
.popzone span.Playbtn1{position:absolute;right:16px;top:0;}
.popzone span.Stopbtn1{position:absolute;right:0;top:0;}


/* tabArea */
#indexArea .tabArea{position:relative;overflow:hidden;width:340px;height:165px;margin-left:14px;margin-right:7px;background:url('/pdqm/images/pdqm/common/main/bg_tabArea_line.gif') repeat-x 0 30px;}
#indexArea .tabArea dl{float:left;display:inline;}
#indexArea .tabArea .more{position:absolute;top:-15px;right:5px;_right:15px;}
#indexArea .list{position:absolute;top:0;left:0;width:310px;height:134px;margin-top:31px;padding:15px;background:url('/pdqm/images/pdqm/common/main/bg_boardBox.gif') no-repeat 0 0;}
#indexArea .list li{overflow:hidden;width:100%;padding-left:8px;line-height:1.6;background:url('/pdqm/images/pdqm/common/icon/bult_1.gif') no-repeat 0 8px;}
#indexArea .list li.first{font-weight:bold;}
#indexArea .list li span{float:right;color:#999;font-weight:normal;}
#indexArea .list li a{float:left;width:220px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;-o-text-overow:ellipsis;}
#indexArea .list li.new a{padding-right:15px;background:url('/pdqm/images/pdqm/common/icon/icon_new.gif') no-repeat right center;}
#indexArea .list li input{float:left;width:240px;line-height:1.6;text-align:left;cursor:pointer;_cursor:hand;border:0;background:#fff;}
#indexArea .list li.new input{background:url('/pdqm/images/pdqm/common/icon/icon_new.gif') no-repeat right center;border:0 none;}

/* QR코드 */
.qrcode{width:113px;}
.qrcode h2, .qrcode p{line-height:0;font-size:0;}

/* 전문가칼럼 */
.specialBox{width:200px;height:121px;margin-bottom:10px;}
/* 트위터 */
.twitterBox{width:200px;height:265px;background:url('/pdqm/images/pdqm/common/main/box_twitter.gif') no-repeat 0 0;}
/* 오른쪽배너영역 */
.banner li{margin-top:7px;}
/* 관련사이트 */
.bannerBox{overflow:hidden;width:986px;height:29px;padding:8px 40px 7px;background:url('/pdqm/images/pdqm/common/main/bannerLink_box.gif') no-repeat 0 0;}
.bannerBox li{float:left;margin-right:53px;}

/* ============ popup ================================================================== */
#popWrap{width:100%;height:100%;margin:0;padding:0;background:#fff;}
.popArea{width:100%;background:url('/pdqm/images/pdqm/common/member/pop_bg_tit.gif') repeat-x 0 0;}
.popArea h2{padding:15px 25px 14px;}
.pop_con{margin:30px 25px;}